      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Andy.X.Runtime.Model\Andy.X.Runtime.Model.csproj" />
  </ItemGroup>

