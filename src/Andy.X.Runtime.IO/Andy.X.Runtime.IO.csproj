<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="data\package\packages_installed.json" />
    <None Remove="data\settings\andyx_config.json" />
    <None Remove="data\settings\artifacts_config.json" />
    <None Remove="data\settings\cluster.json" />
    <None Remove="data\settings\packages_config.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="data\package\packages_installed.json">
